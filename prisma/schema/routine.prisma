model Routine {
    id          String      @id @default(cuid())
    name        String
    fileUrl     String
    type        RoutineType
    createdById String?
    createdBy   User?       @relation("RoutineCreatedBy", fields: [createdById], references: [id])
    createdAt   DateTime    @default(now())
    updatedAt   DateTime    @updatedAt()

    @@map("routines")
}
