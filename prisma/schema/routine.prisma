model Routine {
  id          String      @id @default(cuid())
  name        String
  fileUrl     String
  type        RoutineType
  batchId     String?
  batch       Batch?      @relation(fields: [batchId], references: [id])
  createdById String?
  createdBy   User?       @relation("RoutineCreatedBy", fields: [createdById], references: [id])
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt()
  isDeleted   Boolean     @default(false)

  @@map("routines")
}
